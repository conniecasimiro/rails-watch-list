<div class="container">

  <h1>My Lists</h1>
  <%@lists.each do |list| %>
    <%= cl_image_tag list.photo.key, height: 300, width: 400, crop: :fill %>
    <h3><%= list.name %></h3>
    <%= link_to "See details", list_path(list), class: "btn btn-primary"%>
  <% end %>

  <ul class="list-unstyled mt-3"><li>
    <%= link_to "New list", new_list_path, class: "btn btn-warning btn-lg"%>
  </li></ul>
</div>
